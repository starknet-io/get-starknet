{
  "name": "@starknet-io/get-starknet-core",
  "version": "5.0.0-beta.0",
  "keywords": [
    "starknet",
    "starkware",
    "l2",
    "zk",
    "rollup",
    "wallet",
    "dapp"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./wallets": {
      "types": "./dist/wallets.d.ts",
      "import": "./dist/wallets.mjs",
      "require": "./dist/wallets.cjs",
      "default": "./dist/wallets.mjs"
    },
    "./virtual-wallet": {
      "types": "./dist/virtual-wallet.d.ts",
      "import": "./dist/virtual-wallet.mjs",
      "require": "./dist/virtual-wallet.cjs",
      "default": "./dist/virtual-wallet.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "lint:check": "biome check .",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "@starknet-io/get-starknet-discovery": "workspace:^",
    "@starknet-io/get-starknet-virtual-wallet": "workspace:^",
    "@starknet-io/get-starknet-wallets": "workspace:^",
    "@starknet-io/get-starknet-wallet-standard": "workspace:^"
  }
}
